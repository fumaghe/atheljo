{
  "name": "aiops-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx concurrently \"npm run dev:server\" \"npm run dev:client\" \"npm run dev:pdf\"",
    "dev:server": "cd backend && cross-env PORT=5000 nodemon --watch server.js server.js",
    "dev:client": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vite",
    "dev:pdf": "cross-env PDF_PORT=5174 node src/pdfServer.js",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:local:server": "cd backend && cross-env PORT=5000 nodemon --legacy-watch server.js",
    "dev:local:client": "vite",
    "dev:local": "npx concurrently \"npm run dev:local:server\" \"npm run dev:local:client\""
  },
